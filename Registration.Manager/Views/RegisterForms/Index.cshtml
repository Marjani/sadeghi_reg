        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsPaied)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PaiedOn)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
